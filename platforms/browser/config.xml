<?xml version='1.0' encoding='utf-8'?>
<widget id="rocks.guitarlearning.sft" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Simple Fretboard Trainer</name>
    <description>
        Simple and free app that will help you memorize all of the notes on your fretboard.
    </description>
    <author email="jackrivers.studio@gmail.com" href="http://sft.guitarlearning.rocks/">
        Jack Rivers Studio
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="orientation" value="default" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
